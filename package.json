{
  "name": "vue-directive-mask",
  "description": "Simple mask input directive for Vue.js",
  "keywords": [
    "vue",
    "vue-directive"
  ],
  "license": "MIT",
  "version": "1.2.3",
  "author": "Eduard Nikolenko <mail@eduardnikolenko.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vuejs-community/vue-directive-mask.git"
  },
  "files": [
    "dist/vue-directive-mask.cjs.d.ts",
    "dist/vue-directive-mask.cjs.js",
    "dist/vue-directive-mask.esm.d.ts",
    "dist/vue-directive-mask.esm.js"
  ],
  "main": "dist/vue-directive-mask.cjs.js",
  "module": "dist/vue-directive-mask.esm.js",
  "types": "dist/vue-directive-mask.esm.d.ts",
  "scripts": {
    "build": "npm run build:esm && npm run build:cjs",
    "build:cjs": "rollup --config rollup.config.js --format cjs --file dist/vue-directive-mask.cjs.js",
    "build:esm": "rollup --config rollup.config.js --format esm --file dist/vue-directive-mask.esm.js",
    "lint": "tslint -p tsconfig.json -c tslint.json"
  },
  "devDependencies": {
    "@rollup/plugin-json": "^4.0.2",
    "@wessberg/rollup-plugin-ts": "^1.2.21",
    "rollup": "^2.1.0",
    "tslint": "^6.1.0",
    "typescript": "^3.8.3",
    "vue": "^2.6.11"
  },
  "peerDependencies": {
    "vue": "^2.0.0"
  }
}
